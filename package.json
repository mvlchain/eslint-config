{
  "name": "@mvlchain/eslint-config",
  "version": "1.0.3",
  "description": "Common ESLint config used in MVL FE projects.",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "lint:fix": "./node_modules/.bin/eslint --fix"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "yarn run lint:fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mvlchain/eslint-config.git"
  },
  "keywords": [
    "eslint"
  ],
  "author": "mvlchain",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mvlchain/eslint-config/issues"
  },
  "homepage": "https://github.com/mvlchain/eslint-config#readme",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "devDependencies": {
    "@mvlchain/prettier-config": "^1.0.0",
    "eslint": "^8.19.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "4.8.4"
  },
  "peerDependencies": {
    "eslint": ">=7"
  }
}
